{
    "name": "usox/circulon",
    "homepage": "https://github.com/usox/circulon",
    "description": "A PHP dependency resolver",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "references",
        "dependencytree",
        "dependency"
    ],
    "authors": [
        {
            "name": "Daniel Jakob",
            "email": "github@usox.org",
            "homepage": "https://usox.org"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Usox\\Circulon\\": "src/Circulon"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit -c phpunit.xml.dist"
    }
}
